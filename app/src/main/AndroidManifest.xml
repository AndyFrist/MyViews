<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.huangwenpei.myview">

    <!-- 二维码权限 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.autofocus" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.BaseActivity" />
        <activity android:name=".Activity.SlidemenuActivity" />
        <activity android:name=".Activity.ChartLineActivity" />
        <activity android:name=".Activity.OverScrollyActivity" />
        <activity android:name=".Activity.PullableScrollviewActivity" />
        <activity android:name=".Activity.SwipeDeleteActivity" />
        <activity android:name=".zxing.activity.CaptureActivity" />
        <activity android:name=".Activity.CircleActivity" />
        <activity android:name=".Activity.CircleBitmapActivity" />
        <activity android:name=".Activity.DragItemActivity" />
        <activity android:name=".Activity.MenuManageActivity" />
        <activity android:name=".Activity.VerticalDragLayoutActivity" />
        <activity android:name=".Activity.SearchActivity" />
        <activity android:name=".Activity.HeadActivity"></activity>
    </application>

</manifest>